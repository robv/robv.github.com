@end */

/* @group HOME HEADLINES */

#home_headlines {
	height: 22px;
	border: 1px solid #ccc;
	padding: 5px 5px 5px 55px;
	margin: 20px;
	font-size: 14px;
	background: #fff url(../images/main/news_icon.png) no-repeat 10px 5px;
	overflow: hidden;
}

.home_headlines_title {
	position: absolute;
	color: #FF6600;
	margin: 3px 0 0;
}

.scrollup {
	position: relative;
	width: 500px;
	height: 40px;
	margin: -2px 0 0 140px;
	overflow: hidden;
}

.headline {
	position: absolute;
	top: 210px;
	left: 5px;
	height: 40px;
	width: 500px;
}

.headline a, .headline a:link, .headline a:active, .headline a:visited {
	color: #000;
	text-decoration: none;
}

.headline a:hover {
	color: #ff6600;
}

