}

.w_content_inner_right.signup_box {
	text-align: center;
	margin-top: 35px;
	height: 90px;
}

.w_content_inner_right.signup_box .orange_button {
	font: bold 27px Georgia, "Times New Roman", Times, serif;
	line-height: 34px;
