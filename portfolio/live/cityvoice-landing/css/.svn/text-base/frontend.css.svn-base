.w_content {
	background-color: #fff;
	margin: 20px;
	padding: 20px;
	font: 14px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	color: #777;
}

.w_content_bottom {
	margin-top: 0 !important;
}

.w_content_left {
	width: 415px;
	float: left;
	margin-right: 0;
}

.w_content_right {
	width: 415px;
	float: right;
	margin-left: 0;
}

.w_content h2 {
	font: 28px Georgia, "Times New Roman", Times, serif;
	padding-bottom: 10px;
	color: #000;
}

.w_content h3 {
	font: 18px Georgia, "Times New Roman", Times, serif;
	padding-bottom: 10px;
	color: #000;
}

.w_content p {
	padding-bottom: 10px;
}

.w_content p a, .w_content p a:link, .w_content p a:active, .w_content p a:visited {
	color: #777;
	text-decoration: underline;
}

.w_content p a:hover {
	color: #ff6600;
}

.w_content a, .w_content a:link, .w_content a:active, .w_content a:visited {
	color: #ff6600;
	text-decoration: none;
}

.w_content a:hover {
